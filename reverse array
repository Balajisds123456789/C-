#include<iostream>
using namespace std;

int main() {
int arr[100];
//input

int n;
cout<<"enter size of array"<<endl;
cin>>n;

for(int i=0;i<n;i++)
	{
		
		cout<<"enter elements : " << i << endl;
		cin>>arr[i];
	}

//reverse
int start = 0;
int end = n-1;

while(start<=end)
{
	swap(arr[start],arr[end]);
	start++;
	end--;
	
	}

//print array

for(int i=0;i<n;i++)
	{
		
		cout<<"elements  " << i << "  equal to : "<< arr[i] << endl;
	
	}




}
